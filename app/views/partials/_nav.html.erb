<nav>
	<div class="nav-wrapper">
		<%= link_to :root, class: "brand-logo center" do %>
			<i class="medium material-icons left">textsms</i>Stock Message Service
		<% end %>
		<ul id="nav-mobile" class="left hide-on-med-and-down">
			<% if !current_user %>
				<li>
					<%= link_to new_user_path, class: "" do %>
						<i class="medium material-icons left">perm_contact_calendar</i>Sign Up for SMS Updates
					<% end %>
				</li>
				<li>
					<%= link_to login_path, class: "" do %>
						<i class="medium material-icons left">lock_open</i>Login
					<% end %>
				</li>
			<% else %>
				<li>
					<%= link_to user_path(current_user.id), class: "" do %>
						<i class="medium material-icons left">perm_identity</i>Portfolio 
					<% end %>
				</li>
				<li>
					<%= link_to logout_path, class: "", method: :delete do %>
						<i class="medium material-icons left">lock_outline</i>Logout

					<% end %>
				</li>
			<% end %>
		</ul>
	</div>
